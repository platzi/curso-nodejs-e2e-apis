{
  "name": "my-store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "e2e": "NODE_ENV=e2e jest --config ./jest-e2e.json --verbose --forceExit --runInBand",
    "e2e:ci": "NODE_ENV=ci jest --config ./jest-e2e.json --verbose --forceExit --runInBand",
    "e2e:coverage": "NODE_ENV=e2e jest --config ./jest-e2e.json --verbose --forceExit --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "^28.1.3",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "supertest": "^6.2.4"
  },
  "dependencies": {
    "@faker-js/faker": "7.4.0",
    "@hapi/boom": "10.0.0",
    "bcrypt": "5.0.1",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "nodemailer": "6.7.7",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "8.7.3",
    "sequelize": "6.21.3",
    "sequelize-cli": "6.4.1",
    "umzug": "^3.2.1"
  }
}
